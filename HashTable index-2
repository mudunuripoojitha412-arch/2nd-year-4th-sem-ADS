#include <stdio.h>

#define SIZE 10

int hashtable[SIZE];

/* Initialize hash table */
void init()
{
    for (int i = 0; i < SIZE; i++)
    {
        hashtable[i] = -1;
    }
}

/* Insert using Quadratic Probing */
void insert(int key)
{
    int index = key % SIZE;
    int i = 0;

    while (hashtable[(index + i * i) % SIZE] != -1)
    {
        i++;

        if (i == SIZE)
        {
            printf("Hash table full! Cannot insert %d\n", key);
            return;
        }
    }

    int new_index = (index + i * i) % SIZE;
    hashtable[new_index] = key;

    printf("Inserted %d at index %d\n", key, new_index);
}

/* Display hash table */
void display()
{
    printf("\nHash Table:\n");
    for (int i = 0; i < SIZE; i++)
    {
        printf("%d -> %d\n", i, hashtable[i]);
    }
}

int main()
{
    int keys[] = {23, 43, 13, 27, 39, 14};
    int n = 6;

    init();

    for (int i = 0; i < n; i++)
    {
        insert(keys[i]);
    }

    display();

    return 0;
}
